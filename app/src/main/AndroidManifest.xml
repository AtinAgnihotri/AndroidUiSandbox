<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atinagnihotri.uisandbox">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ui_sandbox"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ui_sandbox"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp">
        <activity android:name=".RecyclerActivity"></activity>
        <activity android:name=".SnackBarCardActivity" />
        <activity android:name=".MaterialDesignActivity" />
        <activity android:name=".XMLTradeMarkActivity" />
        <activity android:name=".ListSpinnerMenuActivity" />
        <activity android:name=".MainActivityBackup" />
        <activity android:name=".ConstraintActivity" />
        <activity android:name=".LinearActivity" />
        <activity android:name=".RelativeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>